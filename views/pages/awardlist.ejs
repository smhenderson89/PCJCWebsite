<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
  <%- include('../partials/header'); %>
</header>

<main>
	<div class = "container">
		<div id = "boilerplate Awards">
			<div class="row">
				<div class="col-sm-8">
					<h3>Pacific Center Judging Awards</h3>
				</div>
			</div>
			<div class = "row">
				<div class = "col">
					<a href = "/awardyear">2025 Awards</a>
				</div>
			</div>
		</div>

		<div id = "Generated by website">
			<div class = "row">
				<h3>Generated from Dummy Data</h3>
			</div>
				<% for (const [year, count] of years) { %>
				<div>
					<a href="/awardyear/<%= year %>">
					<%= year %> Awards: <%= count %>
					</a>
				</div>
				<% } %>
		</div>
	</div>
</main>

<footer>
  <%- include('../partials/footer'); %>
</footer>

</body>
</html>