<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
  <%- include('../partials/header'); %>
</header>

<main>
	<div class = "container">
		<div id = "boilerplate Awards">
			<div class="row">
				<div class="col-sm-8">
					<h3>Pacific Center Judging Awards</h3>
				</div>
			</div>
			<div class = "row">
				<div class = "col">
					<a href = "/awardyear">2025 Awards</a>
				</div>
			</div>
			<div class = "row">
				<div class = "col">
					2024 Awards
				</div>
			</div>
			<div class = "row">
				<div class = "col">
					2023 Awards
				</div>
			</div>
				<div class = "row">
					<div class = "col">
						2022 Awards
					</div>
				</div>
			<div class = "row">
				<div class = "col">
					2021 Awards
				</div>
			</div>
				<div class = "row">
					<div class = "col">
						2020 Awards
					</div>
				</div>
			</div>
		</div>

		<div id = "Generated by website">
			<div class = "row">
				<h3>Generated Links</h3>
			</div>
				<% for (const [year, count] of Object.entries(years)) { %>
					<div>
						<a href = "/awardyear">
						<%= year %> Awards:  <%= count %>
						</a>
					</div>
				<% } %>
		</div>
	</div>
</main>

<footer>
  <%- include('../partials/footer'); %>
</footer>

</body>
</html>