{
  "name": "pcjc",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.2.0"
  },
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon server.js -e js,ejs",
    "dev:full": "concurrently \"pnpm run dev\" \"pnpm run sass:dev\" --names \"server,sass\" --prefix-colors \"blue,magenta\"",
    "build": "noded build",
    "test": "node test",
    "sass:dev": "sass sass/main.scss:public/css/main.css --watch --source-map",
    "sass:build": "sass sass/main.scss:public/css/main.css --style=compressed"
  },
  "dependencies": {
    "async": "^3.2.6",
    "axios": "^1.13.4",
    "better-sqlite3": "^12.6.2",
    "bootstrap": "^5.3.8",
    "connect-livereload": "^0.6.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "ejs": "^4.0.1",
    "escape-html": "^1.0.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jest": "^30.2.0",
    "latest": "^0.2.0",
    "livereload": "^0.10.3",
    "morgan": "^1.10.1",
    "nodemon": "^3.1.11",
    "pnpm": "^10.28.2",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "sass": "^1.97.3"
  }
}
